{
	"name": "vue-notibar",
	"description": "Notification bar for the Vue apps",
	"version": "0.3.2",
	"keywords": [
		"snackbar",
		"notification",
		"toast",
		"vue",
		"plugin"
	],
	"main": "src/index.js",
	"unpkg": "dist/vue-notibar.min.js",
	"license": "MIT",
	"scripts": {
		"build": "rollup --config",
		"watch": "rollup --config --watch",
		"test": "jest",
		"test:appveyor": "jest --reporters=\"default\" --reporters=\"jest-junit\"",
		"eslint": "eslint src/* test/*"
	},
	"author": "Boris Maslennikov <tattdogg@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/b-maslennikov/vue-notibar.git"
	},
	"homepage": "https://github.com/b-maslennikov/vue-notibar",
	"bugs": {
		"url": "https://github.com/b-maslennikov/vue-notibar/issues"
	},
	"dependencies": {
		"vue": "^2.4.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.12.11",
		"@rollup/plugin-eslint": "^8.0.1",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@vue/compiler-sfc": "^3.0.4",
		"@vue/test-utils": "^1.1.2",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.6.3",
		"eslint": "^7.15.0",
		"eslint-plugin-vue": "^7.3.0",
		"jest": "^26.6.3",
		"jest-junit": "^12.0.0",
		"rollup": "^2.35.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-scss": "^3.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-vue": "^5.1.9",
		"vue-eslint-parser": "^7.3.0",
		"vue-jest": "^3.0.7",
		"vue-template-compiler": "^2.6.12"
	},
	"jest-junit": {
		"classNameTemplate": "{classname}",
		"titleTemplate": "{title}"
	}
}
