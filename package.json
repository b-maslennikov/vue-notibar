{
	"name": "vue-notibar",
	"description": "Notification bar for the Vue apps",
	"version": "0.3.5",
	"keywords": [
		"snackbar",
		"notification",
		"toast",
		"vue",
		"plugin"
	],
	"type": "module",
	"main": "src/index.js",
	"typings": "typings/index.d.ts",
	"unpkg": "dist/vue-notibar.min.js",
	"license": "MIT",
	"scripts": {
		"build": "rollup --config --bundleConfigAsCjs",
		"watch": "rollup --config --watch",
		"test": "jest",
		"test:appveyor": "jest --reporters=\"default\" --reporters=\"jest-junit\"",
		"eslint": "eslint src/* test/*"
	},
	"author": "Boris Maslennikov <tattdogg@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/b-maslennikov/vue-notibar.git"
	},
	"homepage": "https://github.com/b-maslennikov/vue-notibar",
	"bugs": {
		"url": "https://github.com/b-maslennikov/vue-notibar/issues"
	},
	"dependencies": {
		"vue": "~2.7.15"
	},
	"devDependencies": {
		"@babel/core": "~7.23.5",
		"@babel/eslint-parser": "~7.23.3",
		"@babel/preset-env": "~7.23.5",
		"@rollup/plugin-eslint": "~9.0.5",
		"@rollup/plugin-node-resolve": "~15.2.3",
		"@rollup/plugin-terser": "~0.4.4",
		"@vue/compiler-sfc": "~2.7.15",
		"@vue/test-utils": "~1.3.6",
		"babel-jest": "~29.7.0",
		"eslint": "~8.55.0",
		"eslint-plugin-vue": "~9.19.2",
		"jest": "~29.7.0",
		"jest-environment-jsdom": "~29.7.0",
		"jest-junit": "~16.0.0",
		"rollup": "~4.6.1",
		"rollup-plugin-cleanup": "~3.2.1",
		"rollup-plugin-scss": "~4.0.0",
		"rollup-plugin-vue2": "~0.8.1",
		"sass": "~1.69.5",
		"vue-jest": "~4.0.1",
		"vue-template-compiler": "~2.7.15"
	},
	"jest-junit": {
		"classNameTemplate": "{classname}",
		"titleTemplate": "{title}"
	}
}
